---
import Layout from '../layouts/Layout.astro';
import Contact from '../components/Contact.tsx';
import Video from '../components/VideoBg.astro';
import { ViewTransitions } from 'astro:transitions';
---

<Layout title="La Carne | Creemos">
    <main class="relative min-h-[655px] overflow-y-hidden">
        <Video/>
        <div class="absolute z-0 inset-0 bg-black bg-opacity-95"></div>
		<Contact client:visible/>
	</main>
    <ViewTransitions class:list={"z-50"} />
</Layout>